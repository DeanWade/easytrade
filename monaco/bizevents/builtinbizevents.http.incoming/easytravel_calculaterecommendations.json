{
  "enabled": true,
  "ruleName": "[ws] easyTravel - /CalculateRecommendations",
  "triggers": [
    {
      "source": {
        "dataSource": "request.path"
      },
      "type": "EQUALS",
      "value": "/CalculateRecommendations",
      "caseSensitive": true
    }
  ],
  "event": {
    "provider": {
      "sourceType": "constant.string",
      "source": "www.easytravel.com"
    },
    "type": {
      "sourceType": "constant.string",
      "source": "com.easytravel.calculate-recommendations"
    },
    "category": {
      "sourceType": "constant.string",
      "source": ""
    },
    "data": [
      {
        "name": "id",
        "source": {
          "sourceType": "constant.string",
          "source": "12345"
        }
      },
      {
        "name": "response",
        "source": {
          "sourceType": "response.statusCode"
        }
      },
      {
        "name": "rsbody",
        "source": {
          "sourceType": "response.body",
          "path": "*"
        }
      },
      {
        "name": "firstIndex",
        "source": {
          "sourceType": "response.body",
          "path": "1"
        }
      },
      {
        "name": "rqbody",
        "source": {
          "sourceType": "request.body",
          "path": "*"
        }
      }
    ]
  }
}